{
  "name": "issue-complete",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "1.1.0",
  "description": "A GitHub app that labels and comments on new issues that don't have a completed task list",
  "author": "Steven Zeck <steven.zeck@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/szeck87/issue-complete.git",
  "scripts": {
    "build": "tsc",
    "start": "probot run ./index.js",
    "test": "jest && standard **/*.ts",
    "fix": "standard **/*.ts --fix"
  },
  "dependencies": {
    "probot": "7.4.0",
    "probot-config": "1.0.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/node": "10.12.11",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "23.6.0",
    "nodemon": "1.18.7",
    "smee-client": "1.0.2",
    "standard": "12.0.1",
    "ts-jest": "23.10.5",
    "typescript": "3.2.1",
    "typescript-eslint-parser": "21.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  }
}
