{
  "name": "issue-complete",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "1.1.0",
  "description": "A GitHub app that labels and comments on new issues that don't have a completed task list",
  "author": "Steven Zeck <steven.zeck@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/szeck87/issue-complete.git",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "probot run ./dist/index.js",
    "test": "jest && standard **/*.ts",
    "fix": "standard **/*.ts --fix"
  },
  "dependencies": {
    "probot": "9.10.1",
    "probot-config": "1.1.0",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "12.12.21",
    "@typescript-eslint/parser": "2.23.0",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "smee-client": "1.1.0",
    "standard": "14.3.1",
    "ts-jest": "25.2.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  }
}
